{
    "name": "theme-jamt",
    "version": "1.1.0",
    "displayName": "Just Another Material Theme",
    "homepage": "https://github.com/k4ustu3h/jamt-vscode",
    "description": "Just Another Material Theme",
    "publisher": "k4ustu3h",
    "license": "MIT",
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "vsce package -o ./bin/jamt.vsix",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Kaustubh Ladiya <k4ustu3h@gmail.com>"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/k4ustu3h/jamt-vscode.git"
    },
    "bugs": {
        "url": "https://github.com/k4ustu3h/jamt-vscode/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "color-theme",
        "dark-theme",
        "dark",
        "jamt",
        "material-design",
        "theme",
        "vscode-theme"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#121212",
        "theme": "dark"
    },
    "contributes": {
        "themes": [
            {
                "label": "Just Another Material Theme",
                "uiTheme": "vs-dark",
                "path": "./theme/jamt.json"
            }
        ]
    },
    "devDependencies": {
        "eslint": "8.37.0",
        "js-yaml": "4.1.0",
        "prettier": "2.8.7",
        "vsce": "2.15.0"
    }
}
